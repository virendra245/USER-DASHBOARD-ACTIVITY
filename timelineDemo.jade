html
  head
    title Timeline demo
    script(type='text/javascript', src='http://www.google.com/jsapi')
    script(type='text/javascript', src='/timeline-2.9.0/timeline.js')
    link(rel='stylesheet', type='text/css', href='/timeline-2.9.0/timeline.css')
    script(src='http://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js')
    link(rel='stylesheet', type='text/css', href='/timeline-2.9.0/timelineDemo.css')
    script.
      window.journeyData = !{JSON.stringify(journeyData)};

    script(type='text/javascript', src='/timeline-2.9.0/timelineDemo.js')

body
    div(style=' \
    width: #{ journeyData[0].Width }px ;\
    height: #{ journeyData[0].Height }px ;\
    border: 1px solid;\
    overflow: hidden;\
    position: relative;\
    background-color: rgb(0, 255, 207);')
      p
        span(style="background-image:url('https://www.google.com/a/proptiger.com/images/logo.gif?alpha=1&service=google_default');background-size:contain;max-width:144px;max-height:40px; margin-top: auto;")
          img(src='https://www.google.com/a/proptiger.com/images/logo.gif?alpha=1&service=google_default', style='max-width:144px;max-height:40px; margin-top:-13')
      .dt
        p  #{ journeyData[0].email } 

      #mytimeline

      form(name='myForm', action='/', onsubmit='return(validate());', method='post')
        .inp
          input(type='text', name='userName', placeholder='Enter email', style='background-color:#23D7AE;margin-left: 72; width:158; height:25;color:black')
        .input_width(style='position: absolute; left:500; top: 400;  display:none')
          input(type='text', name='width1', value='#{ journeyData[0].Width }', style='background-color:#27EA8A;')
          label.top Width
        .input_height(style='position: absolute; left:700; top: 400; display:none')
          input(type='text', name='height1', value='#{ journeyData[0].Height }', style='background-color:#F2E6E3;')
          label.top Height
        .sb
          input(type='submit', value='Submit', style='width: 60;height: 25;')
        .Adjust
          input(type='button', value='Adjust', title='Adjust', onclick='adjustVisibleTimeRangeToAccommodateAllEvents();', style='height:26;width:60;')
        .center
          input(type='button', value='current time', title='current time', onclick='moveToCurrentTime();', style='width:80; height:25')
